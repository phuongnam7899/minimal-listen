{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/media/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    },
    {
      "name": "music",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/music/*.mp3",
          "/assets/music/*.wav",
          "/assets/music/*.ogg",
          "/assets/music/*.m4a",
          "/assets/music/*.aac",
          "/assets/music/*.json"
        ]
      },
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "365d",
        "timeout": "30s",
        "strategy": "freshness"
      }
    }
  ]
}
