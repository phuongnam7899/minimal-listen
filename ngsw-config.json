{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/app.webmanifest",
          "/*.css",
          "/*.js"
        ]
      },
      "cacheQueryOptions": {
        "ignoreVary": true
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/assets/music/**"]
      },
      "cacheQueryOptions": {
        "ignoreVary": true
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": ["/listen", "/", "/**"],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1d"
      }
    }
  ],
  "navigationUrls": ["/**", "!/**/*.*", "!/**/*__*", "!/**/*__*/**"],
  "navigationRequestStrategy": "performance"
}
